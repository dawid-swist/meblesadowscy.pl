<div class="section row">
  <div class="col s12 l12 m12">
    <h4 class="center-align">{{index}}</h4>
    {{#each allReleatedProducts as |item|}}
      <div class="col l2 m4 s6">
        {{product-card product=item linkName='product.gallery' description=item.Title }}
      </div>
    {{/each}}
  </div>
</div>
{{outlet}}
